{
  "manifest_version": 2,

  "name": "URL to QRC",
  "description": "This extension generates a QR Code based on the current url.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "https://ajax.googleapis.com/",
	"tabs"
  ]
}